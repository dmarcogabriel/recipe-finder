      ],
      "prep_time": "20 minutes"
    },
    {
      "id": "4d7b8f54-7c4a-4a8e-94a3-2f2e5874e9d5",
      "name": "Pancakes",
      "ingredients": [
        {
          "id": "b1d7c6e4-6a3d-4c1c-9f8d-2c7e1d9e6f2a",
