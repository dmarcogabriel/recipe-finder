        },
        {
          "id": "a5c7e6d4-5c3e-4d6d-9e6b-7d1e8c5d7b3c",
          "name": "Milk",
          "quantity": "1 cup"
        },
        {
          "id": "d1b6c5f4-7e8d-4c9e-8e7f-1c9d5e1b3a4b",
          "name": "Egg",
          "quantity": "1 unit"
        },
        {
          "id": "c7b9d4e3-6b8d-4f8e-9c7b-2d1e5f6c4a3b",
          "name": "Baking powder",
          "quantity": "2 teaspoons"
        },
        {
          "id": "e7e8d5f6-7b1c-4d6e-9c7d-3a5e6f7b2c1d",
          "name": "Salt",
          "quantity": "1/4 teaspoon"
        },
        {
          "id": "d2f9e6b5-7c1d-4c6f-8e9d-2a6e7d1c5b3a",
          "name": "Sugar",
          "quantity": "2 tablespoons"
        },
        {
          "id": "c1d8e5b6-7d8d-4c9e-9f7d-1e6d3f2c7a4b",
